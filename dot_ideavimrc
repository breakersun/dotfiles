let mapleader=" "

set clipboard+=unnamed
set clipboard+=ideaput
set scrolloff=5
set visualbell noerrorbells
set incsearch hlsearch
set number relativenumber
set ignorecase
set iskeyword+=-
set which-key
set notimeout
set timeoutlen = 5000
Plug 'machakann/vim-highlightedyank'
set ideajoin
set NERDTree